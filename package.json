{
  "name": "groomster",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack",
    "build:server": "webpack --config src/server/webpack.config.js",
    "watch": "concurrently \"npm run watch:client\" \"npm run watch:server\"",
    "watch:client": "webpack -w",
    "watch:server": "webpack --config src/server/webpack.config.js -w",
    "dev": "cross-env PORT=3000 concurrently npm:watch npm:start",
    "start": "nodemon main.js",
    "serve": "cross-env PORT=80 concurrently npm:build npm:start",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
